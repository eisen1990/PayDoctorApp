        </activity>
        <activity android:name=".PrivacySettingPack.PrivacySettingActivity" />
        <activity android:name=".AttendancePack.AttendanceStatusActivity" />
        <activity android:name=".AnnualLeavePack.CheckAnnualLeaveActivity" />
        <activity android:name=".PayStubPack.PayStubActivity" />
